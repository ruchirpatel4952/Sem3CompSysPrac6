// Assume x = static 0, y = static 1
// local 0 will be used as a = x * y

function Mult.main 1   // 1 local var: a
push constant 0
pop local 0            // a = 0

// Load y into temp 0 as counter
push static 1
pop temp 0             // temp 0 = y

label LOOP_START
push temp 0
push constant 0
eq
if-goto END_LOOP       // if y == 0, done

// a = a + x
push local 0
push static 0
add
pop local 0            // store result in a

// y--
push temp 0
push constant 1
sub
pop temp 0             // decrement y

goto LOOP_START

label END_LOOP

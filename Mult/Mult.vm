// Initialize a = 0
    push constant 0
    pop local 0

// Load y (multiplier) into temp 0
    push static 1
    pop temp 0

// Load x (multiplicand) into temp 1
    push static 0
    pop temp 1

label LOOP_START
// if temp 0 == 0, we're done
    push temp 0
    push constant 0
    eq
    if-goto LOOP_END

// a = a + x (temp1)
    push local 0
    push temp 1
    add
    pop local 0

// temp0 = temp0 - 1
    push temp 0
    push constant 1
    sub
    pop temp 0

goto LOOP_START

label LOOP_END

label END
goto END
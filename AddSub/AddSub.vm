function AddSub.calc 0    // no extra local variables

    // ---  (a + b)  ---
    push local 0          // push a
    push local 1          // push b
    add                   // (a + b)

    // ---  subtract old x  ---
    push static 0         // (a + b)   old-x
    sub                   // (a + b) - x

    // ---  store result in x and return it ---
    pop  static 0         // x ‚Üê (a + b) - x
    push static 0         // leave result on stack for caller
    return